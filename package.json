{
  "name": "bagsball",
  "version": "0.1.0",
  "private": true,
  "description": "bagsball ($bagsball) - Flywheel meme coin integrating BagsAMM and DividendsBot on Bags",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "clean": "turbo run clean && rm -rf node_modules",
    "verify": "pnpm --filter @bagsball/worker verify"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "prettier": "^3.1.1",
    "turbo": "^1.11.2",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.12.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
